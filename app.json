{
  "expo": {
    "name": "Forged",
    "slug": "forged",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/forged-logo.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "scheme": "forged",
    "splash": {
      "image": "./assets/forged-logo.png",
      "resizeMode": "cover",
      "backgroundColor": "#C8A84B"
    },
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.forgedapp.forged",
      "appleTeamId": "YT3798448G"
    },
    "android": {
      "package": "com.forgedapp.forged",
      "adaptiveIcon": {
        "foregroundImage": "./assets/forged-logo-android-adaptive.png",
        "backgroundColor": "#4A6741"
      },
      "edgeToEdgeEnabled": true
    },
    "plugins": [
      "expo-router",
      "expo-sqlite",
      [
        "expo-notifications",
        {
          "icon": "./assets/forged-logo.png",
          "color": "#0a0a0a"
        }
      ],
      "expo-dev-client",
      [
        "@sentry/react-native/expo",
        {
          "organization": "yellow-bike-studio",
          "project": "react-native"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
