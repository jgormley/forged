{
  "expo": {
    "name": "Forged",
    "slug": "forged",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/forged-logo.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "scheme": "forged",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#0a0a0a"
    },
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.forgedapp.forged"
    },
    "android": {
      "package": "com.forgedapp.forged",
      "adaptiveIcon": {
        "foregroundImage": "./assets/forged-logo.png",
        "backgroundColor": "#4A6741"
      },
      "edgeToEdgeEnabled": true
    },
    "plugins": [
      "expo-router",
      "expo-sqlite",
      [
        "expo-notifications",
        {
          "icon": "./assets/forged-logo.png",
          "color": "#0a0a0a"
        }
      ],
      "expo-dev-client"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
